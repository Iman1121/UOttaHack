{
    "version": 2,
    "builds": [
      { "src": "/image_ai_script.py", "use": "@vercel/python" }
    ],
    "routes": [
      { 
        "src": "/(.*)", 
        "dest": "image_ai_script.py" ,
        "methods": ["GET", "POST", "PUT", "DELETE", "PATCH", "OPTIONS"],
        "headers": {
          "Access-Control-Allow-Origin": "*"
        }
      }
    ]
  }